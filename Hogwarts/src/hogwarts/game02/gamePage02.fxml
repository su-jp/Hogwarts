<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainPane" prefHeight="900.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hogwarts.game02.GameCon02">
   <children>
      <Label fx:id="lblHome" layoutX="30.0" layoutY="30.0" onMouseClicked="#goHome" styleClass="label-content" text="Go Home" />
      <Label fx:id="lblTitle" alignment="CENTER" layoutY="90.0" prefWidth="1200.0" text="Game Title">
         <font>
            <Font size="48.0" />
         </font>
      </Label>
      <ImageView fx:id="testOrb" fitHeight="150.0" fitWidth="200.0" layoutX="3.0" layoutY="-168.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../resources/img/game02/temp.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
